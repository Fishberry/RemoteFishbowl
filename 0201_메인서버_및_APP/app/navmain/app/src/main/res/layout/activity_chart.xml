                        android:id="@+id/WeeklyChart_Sunday_MinPH"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="15sp"
                        app:layout_constraintStart_toStartOf="@id/WeeklyChart_Sunday"
                        app:layout_constraintTop_toBottomOf="@id/WeeklyChart_Sunday_MaxPH"
                        app:layout_constraintEnd_toEndOf="@id/WeeklyChart_Sunday"/>
