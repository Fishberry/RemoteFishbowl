                    android:id="@+id/saturday_view"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintStart_toEndOf="@id/friday_view"
                    app:layout_constraintEnd_toStartOf="@id/sunday_view"
                    app:layout_constraintTop_toTopOf="@id/weeklyChartLayout"
                    app:layout_constraintBottom_toBottomOf="@id/weeklyChartLayout"
                    app:layout_constraintHorizontal_weight="1"
                    android:background="?attr/selectableItemBackground"/>
